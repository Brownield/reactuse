    "build": "tsup",
    "generate-registry": "tsx --tsconfig ./tsconfig.scripts.json src/registry/init.ts",
    "start:dev": "cross-env COMPONENTS_REGISTRY_URL=http://localhost:5173/reactuse/registry.json node dist/bin.js",
    "start": "node dist/bin.js"
  },
  "dependencies": {
    "@siberiacancode/reactuse": "workspace:*",
    "acorn": "^8.14.0",
    "fs-extra": "^11.2.0",
    "ora": "5.4.1",
    "please-upgrade-node": "^3.2.0",
    "prompts": "^2.4.2",
    "tsconfig-paths": "^4.2.0",
    "yargs": "^17.7.2",
    "chalk": "4.1.2",
    "cosmiconfig": "^9.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.2",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "ts-node": "^10.9.2",
    "tsup": "^6.6.3",
    "tsx": "^4.19.2",
    "cross-env": "^7.0.3",
    "typescript": "^4.9.3"
  }
}
